import Data.List
import System.IO

--Changed from baseball to bmi


bmi4male :: Float -> Float -> Float
bmi4male height weight
	| bmi <= 18.50 = "THIN"
	| bmi <= 24.99 = "NORMAL"
	| bmi <= 29.99 = "FAT"
	| otherwise = "VERY FAT"
	where bmi = weight / ((height/100) ** 2)
	
