import Data.List
import System.IO

--Where clause used with guards with an example of determing a baseball player by their kilo and bats

bmi4male :: Double -> Double -> String

bmi4male kilo meter
	| bmi <= 18.50 = "Thin"
	| bmi <= 24.99	= "Normal"
	| bmi <= 29.99 = "Fat"
	| otherwise = "Very Fat"
	where bmi = kilo / (meter**2)
